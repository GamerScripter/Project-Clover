local ids = {
    189707 -- Natural Disaster Survival
    1962086868 -- Tower Of Hell
    6654918151 -- Trench War
}

if game.PlaceId == ids[1] then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerScripter/Project-Clover/main/Games/Natural-Disaster-Survival"))()
elseif game.PlaceId == ids[2] then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerScripter/Project-Clover/main/Games/Trench-Of-Hell"))()
elseif game.PlaceId == ids[2] then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerScripter/Project-Clover/main/Games/Trench-War"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerScripter/Project-Clover/main/Games/Universal"))()
end
